# Database Configuration
# Replace with your Neon PostgreSQL connection string
DATABASE_URL=postgres://your-username:your-password@your-endpoint/your-database

# Server Configuration
PORT=5000
NODE_ENV=production
FRONTEND_URL=https://your-production-domain.com

# Authentication
JWT_SECRET=REPLACE_WITH_STRONG_PRODUCTION_SECRET

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://your-production-domain.com/api/auth/google/callback

# API Keys for AI Services
OPENROUTER_API_KEY=your_openrouter_api_key_here
HUGGING_FACE_API_KEY=your_hugging_face_api_key_here
