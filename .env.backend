# Backend Environment Variables
PORT=5000
NODE_ENV=production

# Database Configuration
# Replace with your Neon PostgreSQL connection string
DATABASE_URL=postgres://your-username:your-password@your-endpoint/your-database

# Authentication
JWT_SECRET=your_strong_jwt_secret_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://your-backend-url.onrender.com/api/auth/google/callback

# Frontend URL (for redirects)
FRONTEND_URL=https://your-frontend-url.vercel.app

# API Keys for AI Services
OPENROUTER_API_KEY=your_openrouter_api_key_here
HUGGING_FACE_API_KEY=your_hugging_face_api_key_here
